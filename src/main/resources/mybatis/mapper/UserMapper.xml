<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.laydemo.user.mapper.UserMapper">
    <select id="getUser" parameterType="User" resultType="com.example.laydemo.user.entity.User">
        SELECT * FROM user where username = #{username}
    </select>
</mapper>